{
  "number": 93,
  "name": "سُورَةُ الضُّحَىٰ",
  "englishName": "Ad-Dhuhaa",
  "englishNameTranslation": "The Morning Hours",
  "revelationType": "Meccan",
  "numberOfAyahs": 11,
  "ayahs": [
    {
      "number": 6080,
      "text": "Demi waktu matahari sepenggalahan naik,",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6081,
      "text": "dan demi malam apabila telah sunyi (gelap),",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6082,
      "text": "Tuhanmu tiada meninggalkan kamu dan tiada (pula) benci kepadamu.",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6083,
      "text": "Dan sesungguhnya hari kemudian itu lebih baik bagimu daripada yang sekarang (permulaan).",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6084,
      "text": "Dan kelak Tuhanmu pasti memberikan karunia-Nya kepadamu, lalu (hati) kamu menjadi puas.",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6085,
      "text": "Bukankah Dia mendapatimu sebagai seorang yatim, lalu Dia melindungimu?",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6086,
      "text": "Dan Dia mendapatimu sebagai seorang yang bingung, lalu Dia memberikan petunjuk.",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6087,
      "text": "Dan Dia mendapatimu sebagai seorang yang kekurangan, lalu Dia memberikan kecukupan.",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6088,
      "text": "Sebab itu, terhadap anak yatim janganlah kamu berlaku sewenang-wenang.",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6089,
      "text": "Dan terhadap orang yang minta-minta, janganlah kamu menghardiknya.",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    },
    {
      "number": 6090,
      "text": "Dan terhadap nikmat Tuhanmu, maka hendaklah kamu siarkan.",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 535,
      "hizbQuarter": 238,
      "sajda": false
    }
  ],
