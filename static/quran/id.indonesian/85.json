{
  "number": 85,
  "name": "سُورَةُ البُرُوجِ",
  "englishName": "Al-Burooj",
  "englishNameTranslation": "The Constellations",
  "revelationType": "Meccan",
  "numberOfAyahs": 22,
  "ayahs": [
    {
      "number": 5910,
      "text": "Demi langit yang mempunyai gugusan bintang,",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5911,
      "text": "dan hari yang dijanjikan,",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5912,
      "text": "dan yang menyaksikan dan yang disaksikan.",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5913,
      "text": "Binasa dan terlaknatlah orang-orang yang membuat parit,",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5914,
      "text": "yang berapi (dinyalakan dengan) kayu bakar,",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5915,
      "text": "ketika mereka duduk di sekitarnya,",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5916,
      "text": "sedang mereka menyaksikan apa yang mereka perbuat terhadap orang-orang yang beriman.",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5917,
      "text": "Dan mereka tidak menyiksa orang-orang mukmin itu melainkan karena orang-orang mukmin itu beriman kepada Allah Yang Maha Perkasa lagi Maha Terpuji,",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5918,
      "text": "Yang mempunyai kerajaan langit dan bumi; dan Allah Maha Menyaksikan segala sesuatu.",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5919,
      "text": "Sesungguhnya orang-orang yang mendatangkan cobaan kepada orang-orang yang mukmin laki-laki dan perempuan kemudian mereka tidak bertaubat, maka bagi mereka azab Jahannam dan bagi mereka azab (neraka) yang membakar.",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5920,
      "text": "Sesungguhnya orang-orang yang beriman dan mengerjakan amal-amal yang saleh bagi mereka surga yang mengalir di bawahnya sungai-sungai; itulah keberuntungan yang besar.",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5921,
      "text": "Sesungguhnya azab Tuhanmu benar-benar keras.",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5922,
      "text": "Sesungguhnya Dialah Yang menciptakan (makhluk) dari permulaan dan menghidupkannya (kembali).",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5923,
      "text": "Dialah Yang Maha Pengampun lagi Maha Pengasih,",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5924,
      "text": "yang mempunyai 'Arsy, lagi Maha Mulia,",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5925,
      "text": "Maha Kuasa berbuat apa yang dikehendaki-Nya.",
      "numberInSurah": 16,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5926,
      "text": "Sudahkah datang kepadamu berita kaum-kaum penentang,",
      "numberInSurah": 17,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5927,
      "text": "(yaitu kaum) Fir'aun dan (kaum) Tsamud?",
      "numberInSurah": 18,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5928,
      "text": "Sesungguhnya orang-orang kafir selalu mendustakan,",
      "numberInSurah": 19,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5929,
      "text": "padahal Allah mengepung mereka dari belakang mereka.",
      "numberInSurah": 20,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5930,
      "text": "Bahkan yang didustakan mereka itu ialah Al Quran yang mulia,",
      "numberInSurah": 21,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5931,
      "text": "yang (tersimpan) dalam Lauh Mahfuzh.",
      "numberInSurah": 22,
      "juz": 30,
      "manzil": 7,
      "page": 590,
      "ruku": 527,
      "hizbQuarter": 236,
      "sajda": false
    }
  ],
