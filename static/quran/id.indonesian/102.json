      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6171,
      "text": "Janganlah begitu, kelak kamu akan mengetahui (akibat perbuatanmu itu),",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6172,
      "text": "dan janganlah begitu, kelak kamu akan mengetahui.",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6173,
      "text": "Janganlah begitu, jika kamu mengetahui dengan pengetahuan yang yakin,",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 600,
      "ruku": 544,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
