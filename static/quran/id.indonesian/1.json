      "juz": 1,
      "manzil": 1,
      "page": 1,
      "ruku": 1,
      "hizbQuarter": 1,
      "sajda": false
    },
    {
      "number": 3,
      "text": "Maha Pemurah lagi Maha Penyayang.",
