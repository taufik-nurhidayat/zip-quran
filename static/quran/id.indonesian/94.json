      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6094,
      "text": "Dan Kami tinggikan bagimu sebutan (nama)mu,",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6095,
      "text": "Karena sesungguhnya sesudah kesulitan itu ada kemudahan,",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 596,
      "ruku": 536,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6096,
      "text": "sesungguhnya sesudah kesulitan itu ada kemudahan.",
