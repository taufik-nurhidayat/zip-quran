{
  "number": 104,
  "name": "سُورَةُ الهُمَزَةِ",
  "englishName": "Al-Humaza",
  "englishNameTranslation": "The Traducer",
  "revelationType": "Meccan",
  "numberOfAyahs": 9,
  "ayahs": [
    {
      "number": 6180,
      "text": "Kecelakaanlah bagi setiap pengumpat lagi pencela,",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6181,
      "text": "yang mengumpulkan harta dan menghitung-hitung,",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6182,
      "text": "dia mengira bahwa hartanya itu dapat mengkekalkannya,",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6183,
      "text": "sekali-kali tidak! Sesungguhnya dia benar-benar akan dilemparkan ke dalam Huthamah.",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6184,
      "text": "Dan tahukah kamu apa Huthamah itu?",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6185,
      "text": "(yaitu) api (yang disediakan) Allah yang dinyalakan,",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6186,
      "text": "yang (membakar) sampai ke hati.",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6187,
      "text": "Sesungguhnya api itu ditutup rapat atas mereka,",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6188,
      "text": "(sedang mereka itu) diikat pada tiang-tiang yang panjang.",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 601,
      "ruku": 546,
