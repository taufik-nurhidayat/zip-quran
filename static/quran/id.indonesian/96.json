{
  "number": 96,
  "name": "سُورَةُ العَلَقِ",
  "englishName": "Al-Alaq",
  "englishNameTranslation": "The Clot",
  "revelationType": "Meccan",
  "numberOfAyahs": 19,
  "ayahs": [
    {
      "number": 6107,
      "text": "Bacalah dengan (menyebut) nama Tuhanmu Yang menciptakan,",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6108,
      "text": "Dia telah menciptakan manusia dari segumpal darah.",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6109,
      "text": "Bacalah, dan Tuhanmulah Yang Maha Pemurah,",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6110,
      "text": "Yang mengajar (manusia) dengan perantaran kalam,",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6111,
      "text": "Dia mengajar kepada manusia apa yang tidak diketahuinya.",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6112,
      "text": "Ketahuilah! Sesungguhnya manusia benar-benar melampaui batas,",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6113,
      "text": "karena dia melihat dirinya serba cukup.",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6114,
      "text": "Sesungguhnya hanya kepada Tuhanmulah kembali(mu).",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6115,
      "text": "Bagaimana pendapatmu tentang orang yang melarang,",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6116,
      "text": "seorang hamba ketika mengerjakan shalat,",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6117,
      "text": "bagaimana pendapatmu jika orang yang melarang itu berada di atas kebenaran,",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6118,
      "text": "atau dia menyuruh bertakwa (kepada Allah)?",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6119,
      "text": "Bagaimana pendapatmu jika orang yang melarang itu mendustakan dan berpaling?",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6120,
      "text": "Tidaklah dia mengetahui bahwa sesungguhnya Allah melihat segala perbuatannya?",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6121,
      "text": "Ketahuilah, sungguh jika dia tidak berhenti (berbuat demikian) niscaya Kami tarik ubun-ubunnya,",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6122,
      "text": "(yaitu) ubun-ubun orang yang mendustakan lagi durhaka.",
      "numberInSurah": 16,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6123,
      "text": "Maka biarlah dia memanggil golongannya (untuk menolongnya),",
      "numberInSurah": 17,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6124,
      "text": "kelak Kami akan memanggil malaikat Zabaniyah,",
      "numberInSurah": 18,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6125,
      "text": "sekali-kali jangan, janganlah kamu patuh kepadanya; dan sujudlah dan dekatkanlah (dirimu kepada Tuhan).",
      "numberInSurah": 19,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 538,
      "hizbQuarter": 239,
      "sajda": {
        "id": 15,
        "recommended": false,
        "obligatory": true
      }
    }
  ],
