{
  "number": 77,
  "name": "سُورَةُ المُرۡسَلَاتِ",
  "englishName": "Al-Mursalaat",
  "englishNameTranslation": "The Emissaries",
  "revelationType": "Meccan",
  "numberOfAyahs": 50,
  "ayahs": [
    {
      "number": 5623,
      "text": "Demi malaikat-malaikat yang diutus untuk membawa kebaikan,",
      "numberInSurah": 1,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5624,
      "text": "dan (malaikat-malaikat) yang terbang dengan kencangnya,",
      "numberInSurah": 2,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5625,
      "text": "dan (malaikat-malaikat) yang menyebarkan (rahmat Tuhannya) dengan seluas-luasnya,",
      "numberInSurah": 3,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5626,
      "text": "dan (malaikat-malaikat) yang membedakan (antara yang hak dan yang bathil) dengan sejelas-jelasnya,",
      "numberInSurah": 4,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5627,
      "text": "dan (malaikat-malaikat) yang menyampaikan wahyu,",
      "numberInSurah": 5,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5628,
      "text": "untuk menolak alasan-alasan atau memberi peringatan,",
      "numberInSurah": 6,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5629,
      "text": "sesungguhnya apa yang dijanjikan kepadamu itu pasti terjadi.",
      "numberInSurah": 7,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5630,
      "text": "Maka apabila bintang-bintang telah dihapuskan,",
      "numberInSurah": 8,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5631,
      "text": "dan apabila langit telah dibelah,",
      "numberInSurah": 9,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5632,
      "text": "dan apabila gunung-gunung telah dihancurkan menjadi debu,",
      "numberInSurah": 10,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5633,
      "text": "dan apabila rasul-rasul telah ditetapkan waktu (mereka).",
      "numberInSurah": 11,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5634,
      "text": "(Niscaya dikatakan kepada mereka:) \"Sampai hari apakah ditangguhkan (mengazab orang-orang kafir itu)?\"",
      "numberInSurah": 12,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5635,
      "text": "Sampai hari keputusan.",
      "numberInSurah": 13,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5636,
      "text": "Dan tahukah kamu apakah hari keputusan itu?",
      "numberInSurah": 14,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5637,
      "text": "Kecelakaan yang besarlah pada hari itu bagi orang-orang yang mendustakan.",
      "numberInSurah": 15,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5638,
      "text": "Bukankah Kami telah membinasakan orang-orang yang dahulu?",
      "numberInSurah": 16,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5639,
      "text": "Lalu Kami iringkan (azab Kami terhadap) mereka dengan (mengazab) orang-orang yang datang kemudian.",
      "numberInSurah": 17,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5640,
      "text": "Demikianlah Kami berbuat terhadap orang-orang yang berdosa.",
      "numberInSurah": 18,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5641,
      "text": "Kecelakaan yang besarlah pada hari itu bagi orang-orang yang mendustakan.",
      "numberInSurah": 19,
      "juz": 29,
      "manzil": 7,
      "page": 580,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5642,
      "text": "Bukankah Kami menciptakan kamu dari air yang hina?",
      "numberInSurah": 20,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5643,
      "text": "kemudian Kami letakkan dia dalam tempat yang kokoh (rahim),",
      "numberInSurah": 21,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5644,
      "text": "sampai waktu yang ditentukan,",
      "numberInSurah": 22,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5645,
      "text": "lalu Kami tentukan (bentuknya), maka Kami-lah sebaik-baik yang menentukan.",
      "numberInSurah": 23,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5646,
      "text": "Kecelakaan yang besarlah pada hari itu bagi orang-orang yang mendustakan.",
      "numberInSurah": 24,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5647,
      "text": "Bukankah Kami menjadikan bumi (tempat) berkumpul,",
      "numberInSurah": 25,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5648,
      "text": "orang-orang hidup dan orang-orang mati?",
      "numberInSurah": 26,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5649,
      "text": "dan Kami jadikan padanya gunung-gunung yang tinggi, dan Kami beri minum kamu dengan air tawar?",
      "numberInSurah": 27,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5650,
      "text": "Kecelakaan yang besarlah pada hari itu bagi orang-orang yang mendustakan.",
      "numberInSurah": 28,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5651,
      "text": "(Dikatakan kepada mereka pada hari kiamat): \"Pergilah kamu mendapatkan azab yang dahulunya kamu mendustakannya.",
      "numberInSurah": 29,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5652,
      "text": "Pergilah kamu mendapatkan naungan yang mempunyai tiga cabang,",
      "numberInSurah": 30,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5653,
      "text": "yang tidak melindungi dan tidak pula menolak nyala api neraka\".",
      "numberInSurah": 31,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5654,
      "text": "Sesungguhnya neraka itu melontarkan bunga api sebesar dan setinggi istana.",
      "numberInSurah": 32,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5655,
      "text": "Seolah-olah ia iringan unta yang kuning.",
      "numberInSurah": 33,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5656,
      "text": "Kecelakaan yang besarlah pada hari itu bagi orang-orang yang mendustakan.",
      "numberInSurah": 34,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5657,
      "text": "Ini adalah hari, yang mereka tidak dapat berbicara (pada hari itu),",
      "numberInSurah": 35,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5658,
      "text": "dan tidak diizinkan kepada mereka minta uzur sehingga mereka (dapat) minta uzur.",
      "numberInSurah": 36,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5659,
      "text": "Kecelakaan yang besarlah pada hari itu bagi orang-orang yang mendustakan.",
      "numberInSurah": 37,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5660,
      "text": "Ini adalah hari keputusan; (pada hari ini) Kami mengumpulkan kamu dan orang-orang terdahulu.",
      "numberInSurah": 38,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5661,
      "text": "Jika kamu mempunyai tipu daya, maka lakukanlah tipu dayamu itu terhadap-Ku.",
      "numberInSurah": 39,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5662,
      "text": "Kecelakaan yang besarlah pada hari itu bagi orang-orang yang mendustakan.",
      "numberInSurah": 40,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 516,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5663,
      "text": "Sesungguhnya orang-orang yang bertakwa berada dalam naungan (yang teduh) dan (di sekitar) mata-mata air.",
      "numberInSurah": 41,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5664,
      "text": "Dan (mendapat) buah-buahan dari (macam-macam) yang mereka ingini.",
      "numberInSurah": 42,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5665,
      "text": "(Dikatakan kepada mereka): \"Makan dan minumlah kamu dengan enak karena apa yang telah kamu kerjakan\".",
      "numberInSurah": 43,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5666,
      "text": "Sesungguhnya demikianlah Kami memberi balasan kepada orang-orang yang berbuat baik.",
      "numberInSurah": 44,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5667,
      "text": "Kecelakaan yang besarlah pada hari itu bagi orang-orang yang mendustakan.",
      "numberInSurah": 45,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5668,
      "text": "(Dikatakan kepada orang-orang kafir): \"Makanlah dan bersenang-senanglah kamu (di dunia dalam waktu) yang pendek; sesungguhnya kamu adalah orang-orang yang berdosa\".",
      "numberInSurah": 46,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5669,
      "text": "Kecelakaan yang besarlah pada hari itu bagi orang-orang yang mendustakan.",
      "numberInSurah": 47,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5670,
      "text": "Dan apabila dikatakan kepada mereka: \"Rukuklah, niscaya mereka tidak mau ruku'.",
      "numberInSurah": 48,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5671,
      "text": "Kecelakaan yang besarlah pada hari itu bagi orang-orang yang mendustakan.",
      "numberInSurah": 49,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    },
    {
      "number": 5672,
      "text": "Maka kepada perkataan apakah selain Al Quran ini mereka akan beriman?",
      "numberInSurah": 50,
      "juz": 29,
      "manzil": 7,
      "page": 581,
      "ruku": 517,
      "hizbQuarter": 232,
      "sajda": false
    }
  ],
