  "numberOfAyahs": 8,
  "ayahs": [
    {
      "number": 6099,
      "text": "Demi (buah) Tin dan (buah) Zaitun,",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6100,
      "text": "dan demi bukit Sinai,",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6101,
      "text": "dan demi kota (Mekah) ini yang aman,",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6102,
      "text": "sesungguhnya Kami telah menciptakan manusia dalam bentuk yang sebaik-baiknya.",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6103,
      "text": "Kemudian Kami kembalikan dia ke tempat yang serendah-rendahnya (neraka),",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6104,
      "text": "kecuali orang-orang yang beriman dan mengerjakan amal saleh; maka bagi mereka pahala yang tiada putus-putusnya.",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6105,
      "text": "Maka apakah yang menyebabkan kamu mendustakan (hari) pembalasan sesudah (adanya keterangan-keterangan) itu?",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 597,
      "ruku": 537,
      "hizbQuarter": 239,
      "sajda": false
    },
    {
      "number": 6106,
      "text": "Bukankah Allah Hakim yang seadil-adilnya?",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
