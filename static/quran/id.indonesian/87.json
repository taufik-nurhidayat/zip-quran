{
  "number": 87,
  "name": "سُورَةُ الأَعۡلَىٰ",
  "englishName": "Al-A'laa",
  "englishNameTranslation": "The Most High",
  "revelationType": "Meccan",
  "numberOfAyahs": 19,
  "ayahs": [
    {
      "number": 5949,
      "text": "Sucikanlah nama Tuhanmu Yang Maha Tingi,",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5950,
      "text": "yang menciptakan, dan menyempurnakan (penciptaan-Nya),",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5951,
      "text": "dan yang menentukan kadar (masing-masing) dan memberi petunjuk,",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5952,
      "text": "dan yang menumbuhkan rumput-rumputan,",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5953,
      "text": "lalu dijadikan-Nya rumput-rumput itu kering kehitam-hitaman.",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5954,
      "text": "Kami akan membacakan (Al Quran) kepadamu (Muhammad) maka kamu tidak akan lupa,",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5955,
      "text": "kecuali kalau Allah menghendaki. Sesungguhnya Dia mengetahui yang terang dan yang tersembunyi.",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5956,
      "text": "dan Kami akan memberi kamu taufik ke jalan yang mudah,",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5957,
      "text": "oleh sebab itu berikanlah peringatan karena peringatan itu bermanfaat,",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5958,
      "text": "orang yang takut (kepada Allah) akan mendapat pelajaran,",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5959,
      "text": "dan orang-orang yang celaka (kafir) akan menjauhinya.",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5960,
      "text": "(Yaitu) orang yang akan memasuki api yang besar (neraka).",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5961,
      "text": "Kemudian dia tidak akan mati di dalamnya dan tidak (pula) hidup.",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5962,
      "text": "Sesungguhnya beruntunglah orang yang membersihkan diri (dengan beriman),",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5963,
      "text": "dan dia ingat nama Tuhannya, lalu dia sembahyang.",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5964,
      "text": "Tetapi kamu (orang-orang kafir) memilih kehidupan duniawi.",
      "numberInSurah": 16,
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5965,
      "text": "Sedang kehidupan akhirat adalah lebih baik dan lebih kekal.",
      "numberInSurah": 17,
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5966,
      "text": "Sesungguhnya ini benar-benar terdapat dalam kitab-kitab yang dahulu,",
      "numberInSurah": 18,
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    },
    {
      "number": 5967,
      "text": "(yaitu) Kitab-kitab Ibrahim dan Musa",
      "numberInSurah": 19,
      "juz": 30,
      "manzil": 7,
      "page": 592,
      "ruku": 529,
      "hizbQuarter": 237,
      "sajda": false
    }
  ],
