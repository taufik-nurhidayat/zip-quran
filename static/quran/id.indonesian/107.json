{
  "number": 107,
  "name": "سُورَةُ المَاعُونِ",
  "englishName": "Al-Maa'un",
  "englishNameTranslation": "Almsgiving",
  "revelationType": "Meccan",
  "numberOfAyahs": 7,
  "ayahs": [
    {
      "number": 6198,
      "text": "Tahukah kamu (orang) yang mendustakan agama?",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6199,
      "text": "Itulah orang yang menghardik anak yatim,",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6200,
      "text": "dan tidak menganjurkan memberi makan orang miskin.",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6201,
      "text": "Maka kecelakaanlah bagi orang-orang yang shalat,",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6202,
      "text": "(yaitu) orang-orang yang lalai dari shalatnya,",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6203,
      "text": "orang-orang yang berbuat riya,",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
      "hizbQuarter": 240,
      "sajda": false
    },
    {
      "number": 6204,
      "text": "dan enggan (menolong dengan) barang berguna.",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 602,
      "ruku": 549,
