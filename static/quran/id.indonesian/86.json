{
  "number": 86,
  "name": "سُورَةُ الطَّارِقِ",
  "englishName": "At-Taariq",
  "englishNameTranslation": "The Morning Star",
  "revelationType": "Meccan",
  "numberOfAyahs": 17,
  "ayahs": [
    {
      "number": 5932,
      "text": "Demi langit dan yang datang pada malam hari,",
      "numberInSurah": 1,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5933,
      "text": "tahukah kamu apakah yang datang pada malam hari itu?",
      "numberInSurah": 2,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5934,
      "text": "(yaitu) bintang yang cahayanya menembus,",
      "numberInSurah": 3,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5935,
      "text": "tidak ada suatu jiwapun (diri) melainkan ada penjaganya.",
      "numberInSurah": 4,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5936,
      "text": "Maka hendaklah manusia memperhatikan dari apakah dia diciptakan?",
      "numberInSurah": 5,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5937,
      "text": "Dia diciptakan dari air yang dipancarkan,",
      "numberInSurah": 6,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5938,
      "text": "yang keluar dari antara tulang sulbi laki-laki dan tulang dada perempuan.",
      "numberInSurah": 7,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5939,
      "text": "Sesungguhnya Allah benar-benar kuasa untuk mengembalikannya (hidup sesudah mati).",
      "numberInSurah": 8,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5940,
      "text": "Pada hari dinampakkan segala rahasia,",
      "numberInSurah": 9,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5941,
      "text": "maka sekali-kali tidak ada bagi manusia itu suatu kekuatanpun dan tidak (pula) seorang penolong.",
      "numberInSurah": 10,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5942,
      "text": "Demi langit yang mengandung hujan",
      "numberInSurah": 11,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5943,
      "text": "dan bumi yang mempunyai tumbuh-tumbuhan,",
      "numberInSurah": 12,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5944,
      "text": "sesungguhnya Al Quran itu benar-benar firman yang memisahkan antara yang hak dan yang bathil.",
      "numberInSurah": 13,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5945,
      "text": "dan sekali-kali bukanlah dia senda gurau.",
      "numberInSurah": 14,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5946,
      "text": "Sesungguhnya orang kafir itu merencanakan tipu daya yang jahat dengan sebenar-benarnya.",
      "numberInSurah": 15,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5947,
      "text": "Dan Akupun membuat rencana (pula) dengan sebenar-benarnya.",
      "numberInSurah": 16,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    },
    {
      "number": 5948,
      "text": "Karena itu beri tangguhlah orang-orang kafir itu yaitu beri tangguhlah mereka itu barang sebentar.",
      "numberInSurah": 17,
      "juz": 30,
      "manzil": 7,
      "page": 591,
      "ruku": 528,
      "hizbQuarter": 236,
      "sajda": false
    }
  ],
