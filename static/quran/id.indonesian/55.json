{
  "number": 55,
  "name": "سُورَةُ الرَّحۡمَٰن",
  "englishName": "Ar-Rahmaan",
  "englishNameTranslation": "The Beneficent",
  "revelationType": "Medinan",
  "numberOfAyahs": 78,
  "ayahs": [
    {
      "number": 4902,
      "text": "(Tuhan) Yang Maha Pemurah,",
      "numberInSurah": 1,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4903,
      "text": "Yang telah mengajarkan al Quran.",
      "numberInSurah": 2,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4904,
      "text": "Dia menciptakan manusia.",
      "numberInSurah": 3,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4905,
      "text": "Mengajarnya pandai berbicara.",
      "numberInSurah": 4,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4906,
      "text": "Matahari dan bulan (beredar) menurut perhitungan.",
      "numberInSurah": 5,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4907,
      "text": "Dan tumbuh-tumbuhan dan pohon-pohonan kedua-duanya tunduk kepada-Nya.",
      "numberInSurah": 6,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4908,
      "text": "Dan Allah telah meninggikan langit dan Dia meletakkan neraca (keadilan).",
      "numberInSurah": 7,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4909,
      "text": "Supaya kamu jangan melampaui batas tentang neraca itu.",
      "numberInSurah": 8,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4910,
      "text": "Dan tegakkanlah timbangan itu dengan adil dan janganlah kamu mengurangi neraca itu.",
      "numberInSurah": 9,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4911,
      "text": "Dan Allah telah meratakan bumi untuk makhluk(Nya).",
      "numberInSurah": 10,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4912,
      "text": "Di bumi itu ada buah-buahan dan pohon kurma yang mempunyai kelopak mayang.",
      "numberInSurah": 11,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4913,
      "text": "Dan biji-bijian yang berkulit dan bunga-bunga yang harum baunya.",
      "numberInSurah": 12,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4914,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 13,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4915,
      "text": "Dia menciptakan manusia dari tanah kering seperti tembikar,",
      "numberInSurah": 14,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4916,
      "text": "dan Dia menciptakan jin dari nyala api.",
      "numberInSurah": 15,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4917,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 16,
      "juz": 27,
      "manzil": 7,
      "page": 531,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4918,
      "text": "Tuhan yang memelihara kedua tempat terbit matahari dan Tuhan yang memelihara kedua tempat terbenamnya",
      "numberInSurah": 17,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4919,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 18,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4920,
      "text": "Dia membiarkan dua lautan mengalir yang keduanya kemudian bertemu,",
      "numberInSurah": 19,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4921,
      "text": "antara keduanya ada batas yang tidak dilampaui masing-masing.",
      "numberInSurah": 20,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4922,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 21,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4923,
      "text": "Dari keduanya keluar mutiara dan marjan.",
      "numberInSurah": 22,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4924,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 23,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4925,
      "text": "Dan kepunyaan-Nya lah bahtera-bahtera yang tinggi layarnya di lautan laksana gunung-gunung.",
      "numberInSurah": 24,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4926,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 25,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 466,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4927,
      "text": "Semua yang ada di bumi itu akan binasa.",
      "numberInSurah": 26,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4928,
      "text": "Dan tetap kekal Dzat Tuhanmu yang mempunyai kebesaran dan kemuliaan.",
      "numberInSurah": 27,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4929,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 28,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4930,
      "text": "Semua yang ada di langit dan bumi selalu meminta kepada-Nya. Setiap waktu Dia dalam kesibukan.",
      "numberInSurah": 29,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4931,
      "text": "Maka nikmat Rabb-mu yang manakah yang kamu dustakan?",
      "numberInSurah": 30,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4932,
      "text": "Kami akan memperhatikan sepenuhnya kepadamu hai manusia dan jin.",
      "numberInSurah": 31,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4933,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 32,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4934,
      "text": "Hai jama'ah jin dan manusia, jika kamu sanggup menembus (melintasi) penjuru langit dan bumi, maka lintasilah, kamu tidak dapat menembusnya kecuali dengan kekuatan.",
      "numberInSurah": 33,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4935,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 34,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4936,
      "text": "Kepada kamu, (jin dan manusia) dilepaskan nyala api dan cairan tembaga maka kamu tidak dapat menyelamatkan diri (dari padanya).",
      "numberInSurah": 35,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4937,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 36,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4938,
      "text": "Maka apabila langit telah terbelah dan menjadi merah mawar seperti (kilapan) minyak.",
      "numberInSurah": 37,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4939,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 38,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4940,
      "text": "Pada waktu itu manusia dan jin tidak ditanya tentang dosanya.",
      "numberInSurah": 39,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4941,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 40,
      "juz": 27,
      "manzil": 7,
      "page": 532,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4942,
      "text": "Orang-orang yang berdosa dikenal dengan tanda-tandannya, lalu dipegang ubun-ubun dan kaki mereka.",
      "numberInSurah": 41,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4943,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 42,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4944,
      "text": "Inilah neraka Jahannam yang didustakan oleh orang-orang berdosa.",
      "numberInSurah": 43,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4945,
      "text": "Mereka berkeliling di antaranya dan di antara air mendidih yang memuncak panasnya.",
      "numberInSurah": 44,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4946,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 45,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 467,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4947,
      "text": "Dan bagi orang yang takut akan saat menghadap Tuhannya ada dua surga.",
      "numberInSurah": 46,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4948,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?,",
      "numberInSurah": 47,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4949,
      "text": "kedua surga itu mempunyai pohon-pohonan dan buah-buahan.",
      "numberInSurah": 48,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4950,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 49,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4951,
      "text": "Di dalam kedua surga itu ada dua buah mata air yang mengalir",
      "numberInSurah": 50,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4952,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 51,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4953,
      "text": "Di dalam kedua surga itu terdapat segala macam buah-buahan yang berpasangan.",
      "numberInSurah": 52,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4954,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 53,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4955,
      "text": "Mereka bertelekan di atas permadani yang sebelah dalamnya dari sutera. Dan buah-buahan di kedua surga itu dapat (dipetik) dari dekat.",
      "numberInSurah": 54,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4956,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 55,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4957,
      "text": "Di dalam surga itu ada bidadari-bidadari yang sopan menundukkan pandangannya, tidak pernah disentuh oleh manusia sebelum mereka (penghuni-penghuni surga yang menjadi suami mereka), dan tidak pula oleh jin.",
      "numberInSurah": 56,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4958,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 57,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4959,
      "text": "Seakan-akan bidadari itu permata yakut dan marjan.",
      "numberInSurah": 58,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4960,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 59,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4961,
      "text": "Tidak ada balasan kebaikan kecuali kebaikan (pula).",
      "numberInSurah": 60,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4962,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 61,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4963,
      "text": "Dan selain dari dua surga itu ada dua surga lagi",
      "numberInSurah": 62,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4964,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 63,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4965,
      "text": "Kedua surga itu (kelihatan) hijau tua warnanya.",
      "numberInSurah": 64,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4966,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 65,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4967,
      "text": "Di dalam kedua surga itu ada dua buah mata air yang memancar.",
      "numberInSurah": 66,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4968,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 67,
      "juz": 27,
      "manzil": 7,
      "page": 533,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4969,
      "text": "Di dalam keduanya (ada macam-macam) buah-buahan dan kurma serta delima.",
      "numberInSurah": 68,
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
      "number": 4970,
      "text": "Maka nikmat Tuhan kamu yang manakah yang kamu dustakan?",
      "numberInSurah": 69,
      "juz": 27,
      "manzil": 7,
      "page": 534,
      "ruku": 468,
      "hizbQuarter": 213,
      "sajda": false
    },
    {
